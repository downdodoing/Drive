package com.example.util.map;

import java.util.Date;
/**
 * 定位数据
 * @author lenovo
 *
 */
public class MapInfo {
	private String province, city, district, road, poiName,date,address,country;
	private double latitude, longitude;

	public MapInfo() {
		super();
		this.province = null;
		this.city = null;
		this.district = null;
		this.road = null;
		this.poiName = null;
		this.latitude = 0;
		this.longitude = 0;
		this.date = null;
		this.address=null;
		this.country=null;
	}

	public MapInfo(String province, String city, String district, String road,
			String poiName, Double latitude, Double longitude, String date,String address,String country) {
		super();
		this.province = province;
		this.city = city;
		this.district = district;
		this.road = road;
		this.poiName = poiName;
		this.latitude = latitude;
		this.longitude = longitude;
		this.date = date;
		this.address=address;
		this.country=country;
	}

	public String getProvince() {
		return province;
	}

	public void setProvince(String province) {
		this.province = province;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getDistrict() {
		return district;
	}

	public void setDistrict(String district) {
		this.district = district;
	}

	public String getRoad() {
		return road;
	}

	public void setRoad(String road) {
		this.road = road;
	}

	public String getPoiName() {
		return poiName;
	}

	public void setPoiName(String poiName) {
		this.poiName = poiName;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getCountry() {
		return country;
	}

	public void setCountry(String country) {
		this.country = country;
	}

	public double getLatitude() {
		return latitude;
	}

	public void setLatitude(double latitude) {
		this.latitude = latitude;
	}

	public double getLongitude() {
		return longitude;
	}

	public void setLongitude(double longitude) {
		this.longitude = longitude;
	}

}
